<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TempActivity"
    android:orientation="vertical"
    >

    <Button
        android:onClick="gotoLoginActivity"
        android:id="@+id/gotoLoginActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="자영 시작페이지"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="84dp" />

    <Button
        android:onClick="gotoAdminActivity"
        android:id="@+id/gotoAdminActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin페이지"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="26dp" />

    <Button
        android:onClick="gotoBeaconActivity"
        android:id="@+id/gotoBeaconActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비콘으로가자자자"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="1dp"/>

    <Button
        android:onClick="gotoQuerySampleActivity"
        android:id="@+id/gotoQuerySampleActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="쿼리 사용법"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="26dp" />

</LinearLayout>